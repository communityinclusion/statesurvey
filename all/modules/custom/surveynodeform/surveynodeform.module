  $query1 = new EntityFieldQuery();
    $query1->entityCondition('entity_type', 'node')
      ->entityCondition('bundle', "individual")
        ->propertyCondition('status', NODE_PUBLISHED)
        ->fieldCondition('og_group_ref', 'target_id', $provid);
