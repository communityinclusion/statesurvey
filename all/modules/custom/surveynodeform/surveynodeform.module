             ${$value . 'pct'} = 0;

            if ($key < 3){
               ${$value . 'hrs'} = 0;
              ${$value . 'hrspct'} = 0;
             ${$value . 'meanhrs'} = 0;
             ${$value . 'check'} = false;
             }



